{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "FullBucketAndObjectAccess",
      "Effect": "Allow",
      "Action": "s3:*",
      "Resource": [
        "arn:aws:s3:::orion-datahub-matches-dev",
        "arn:aws:s3:::orion-datahub-payments-dev",
        "arn:aws:s3:::orion-datahub-statements-dev",
        "arn:aws:s3:::orion-datahub-matches-dev/*",
        "arn:aws:s3:::orion-datahub-payments-dev/*",
        "arn:aws:s3:::orion-datahub-statements-dev/*"
      ]
    }
  ]
}
